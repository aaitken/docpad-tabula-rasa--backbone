{
  "name": "no-skeleton.docpad",
  "version": "0.1.0",
  "description": "New DocPad project without using a skeleton",
  "engines": {
    "node": "0.10",
    "npm": "1.3"
  },
  "dependencies": {
    "brfs": "^1.3.0",
    "browserify": "^8.1.3",
    "browserify-shim": "^3.8.2",
    "docpad": "~6.69.2",
    "docpad-plugin-coffeekup": "~2.2.0",
    "docpad-plugin-coffeescript": "~2.4.0",
    "docpad-plugin-cson": "~2.1.0",
    "docpad-plugin-frontend": "^2.0.1",
    "docpad-plugin-grunt": "~2.1.1",
    "docpad-plugin-partials": "~2.9.1",
    "docpad-plugin-sass": "~2.7.3",
    "grunt-browserify": "^3.3.0",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-cssmin": "^0.11.0",
    "grunt-contrib-uglify": "^0.7.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "brfs"
    ]
  },
  "browserify-shim": {
    "jQuery": "global:jQuery",
    "Backbone": "global:Backbone",
    "Underscore": "global:_"
  },
  "main": "node_modules/docpad/bin/docpad-server",
  "scripts": {
    "start": "node_modules/docpad/bin/docpad-server"
  }
}
